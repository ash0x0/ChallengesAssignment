M (gii)
no_of_sem=0;
while ((x>0 )||(y>0)){
if (x>6)
no_of_sem = no_of_sem+1;
if (y>6)
no_of_sem = no_of_sem+1;
x=x-6;
y=y-6;
}
cout<<no_of_sem;
